/**
 * Returns product of all numbers in a numeric array.
 * Returns null if param is null or array is empty.
 * @param {Array} values - The array containing the items.
 */
function product(values) {
  var sum = 1;
  for (var i = 0; i < values.length; i++) {
      sum = sum * values[i];
  }
  return sum || null;
}
